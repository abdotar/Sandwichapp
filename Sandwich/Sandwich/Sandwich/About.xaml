<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Sandwich.About"
             NavigationPage.HasNavigationBar="false">
    <ContentPage.Content>
        <StackLayout x:Name="content">

            <StackLayout x:Name="top" Padding="10">
            </StackLayout>

            <StackLayout x:Name="center" VerticalOptions="FillAndExpand" HorizontalOptions="FillAndExpand">
            </StackLayout>

            <StackLayout x:Name="buttom">
            </StackLayout>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>